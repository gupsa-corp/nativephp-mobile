APP_NAME="Gupsa Mobile"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:9997

APP_LOCALE=ko
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

# NativePHP Mobile Configuration (add when you have license)
# NATIVEPHP_APP_ID=com.gupsa.mobile
# NATIVEPHP_APP_VERSION=1.0.0
# NATIVEPHP_APP_VERSION_CODE=1

# Database (SQLite for mobile)
DB_CONNECTION=sqlite
DB_DATABASE=database/database.sqlite

# Logging
LOG_CHANNEL=single
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Session
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Cache
CACHE_STORE=file
CACHE_PREFIX=

# Queue
QUEUE_CONNECTION=database

# Filesystem
FILESYSTEM_DISK=local

# Notification
MAIL_MAILER=log